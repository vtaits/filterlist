{
  "type": "module",
  "name": "@vtaits/filterlist",
  "version": "0.3.1",
  "description": "Creating lists with filters, sotring, paginatinon, endless scroll etc",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": "git@github.com:vtaits/filterlist.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "keywords": [
    "filterlist",
    "filter",
    "sort",
    "pagination",
    "vanilla"
  ],
  "scripts": {
    "build": "tsup src/index.ts --sourcemap --format esm,cjs --dts --legacy-output",
    "lint": "biome check src",
    "lint:fix": "biome check src --apply-unsafe",
    "format": "biome format src --write",
    "test:ts": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:unit:dev": "vitest",
    "test": "yarn test:unit",
    "prepare": "yarn build"
  },
  "dependencies": {
    "mitt": "^3.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.3.3",
    "@storybook/react": "^7.5.3",
    "@types/qs": "^6.9.10",
    "@types/react": "^18.2.37",
    "@vtaits/react-router-dom-fake-browser": "^1.0.0",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-router-dom": "^6.19.0",
    "tsup": "^7.3.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
}
