{
  "type": "module",
  "name": "@vtaits/filterlist",
  "version": "0.3.0",
  "description": "Creating lists with filters, sotring, paginatinon, endless scroll etc",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": "git@github.com:vtaits/filterlist.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "keywords": [
    "filterlist",
    "filter",
    "sort",
    "pagination",
    "vanilla"
  ],
  "scripts": {
    "build": "tsup src/index.ts --sourcemap --format esm,cjs --dts --legacy-output",
    "lint": "rome check src",
    "format": "rome check --apply-unsafe src && rome format --write src",
    "test:ts": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:unit:dev": "vitest",
    "test": "yarn test:unit",
    "prepare": "yarn build"
  },
  "dependencies": {
    "mitt": "^3.0.1"
  },
  "devDependencies": {
    "@storybook/react": "^7.4.0",
    "@types/qs": "^6.9.8",
    "@types/react": "^18.2.21",
    "@types/react-router-dom": "^5.3.3",
    "@vtaits/react-router-dom-fake-browser": "^1.0.0",
    "cross-env": "^7.0.3",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "rome": "^12.1.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  }
}
