{
  "type": "module",
  "name": "@vtaits/filterlist",
  "version": "3.0.0-signals.0",
  "description": "Creating lists with filters, sotring, paginatinon, endless scroll etc",
  "files": [
    "dist"
  ],
  "main": "./dist/core.js",
  "module": "./dist/core.js",
  "types": "dist/core.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js"
    },
    "./datastore/string": {
      "types": "./dist/datastore_string.d.ts",
      "import": "./dist/datastore_string.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vtaits/filterlist.git"
  },
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "keywords": [
    "filterlist",
    "filter",
    "sort",
    "pagination",
    "vanilla"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "biome check src",
    "lint:fix": "biome check src --write --unsafe",
    "format": "biome format src --write",
    "test:ts": "tsc --noEmit",
    "test:unit": "bun test --coverage",
    "test:unit:dev": "bun test --watch",
    "test": "bun run test:unit",
    "prepare": "bun run build"
  },
  "dependencies": {
    "@types/qs": "^6.14.0",
    "qs": "^6.14.0",
    "signal-polyfill": "^0.2.1",
    "sleep-promise": "^9.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "@happy-dom/global-registrator": "^18.0.1",
    "@storybook/react-vite": "^9.0.12",
    "@swc/core": "^1.12.3",
    "@testing-library/dom": "^10.4.0",
    "@types/react": "^19.1.8",
    "@vtaits/react-router-dom-fake-browser": "^1.0.0",
    "@vtaits/react-signals": "^1.0.1",
    "@vtaits/use-latest": "^1.3.0",
    "happy-dom": "^18.0.1",
    "react": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "tsup": "^8.5.0",
    "typescript": "5.8.3",
    "vite": "^6.3.5"
  }
}
