{
  "type": "module",
  "name": "@vtaits/filterlist",
  "version": "0.3.0",
  "description": "Creating lists with filters, sotring, paginatinon, endless scroll etc",
  "files": [
    "dist"
  ],
  "main": "./dist/filterlist.umd.cjs",
  "module": "./dist/filterlist.js",
  "exports": {
    ".": {
      "import": "./dist/filterlist.js",
      "require": "./dist/filterlist.umd.cjs"
    }
  },
  "typings": "dist/ts/index.d.ts",
  "repository": "git@github.com:vtaits/filterlist.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "keywords": [
    "filterlist",
    "filter",
    "sort",
    "pagination",
    "vanilla"
  ],
  "scripts": {
    "build:js": "vite build",
    "build:ts": "tsc --declaration --emitDeclarationOnly",
    "build": "yarn build:js && yarn build:ts",
    "lint": "eslint src --ext .ts,.tsx --ignore-path ../../.gitignore",
    "test:ts": "tsc -p ./tsconfig.validate.json --noEmit",
    "test:unit": "jest",
    "test": "yarn test:unit",
    "prepare": "yarn build"
  },
  "dependencies": {
    "mitt": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.24",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/parser": "^5.41.0",
    "@vtaits/react-router-dom-fake-browser": "alpha",
    "cross-env": "^7.0.3",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.2.2",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4",
    "vite": "3.1.8"
  }
}
