{
  "name": "@vtaits/qwik-filterlist",
  "version": "2.0.0",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vtaits/filterlist.git"
  },
  "main": "./lib/index.qwik.mjs",
  "devDependencies": {
    "@biomejs/biome": "^2.3.2",
    "@builder.io/qwik": "^1.17.1",
    "@typescript/native-preview": "^7.0.0-dev.20251029.1",
    "@vtaits/filterlist": "^3.0.0",
    "typescript": "5.9.3",
    "vite": "^7.1.12"
  },
  "peerDependencies": {
    "@builder.io/qwik": "^1.5.1",
    "@vtaits/filterlist": "^3.0.0"
  },
  "exports": {
    ".": {
      "types": "./lib-types/index.d.ts",
      "import": "./lib/index.qwik.mjs",
      "require": "./lib/index.qwik.cjs"
    }
  },
  "description": "Qwik wrapper above @vtaits/filterlist",
  "files": [
    "lib",
    "lib-types"
  ],
  "keywords": [
    "filterlist",
    "filter",
    "sort",
    "pagination",
    "qwik"
