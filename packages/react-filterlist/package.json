{
  "type": "module",
  "name": "@vtaits/react-filterlist",
  "version": "2.0.1",
  "description": "React wrapper above @vtaits/filterlist",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vtaits/filterlist.git"
  },
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "keywords": [
    "filterlist",
    "filter",
    "sort",
    "pagination",
    "react"
  ],
  "scripts": {
    "build": "tsup src/index.ts --sourcemap --format esm,cjs --dts",
    "lint": "biome check src",
    "lint:fix": "biome check src --write --unsafe",
    "format": "biome format src --write",
    "test:ts": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:unit:dev": "vitest",
    "test": "yarn test:unit",
    "prepare": "yarn build"
  },
  "peerDependencies": {
    "@vtaits/filterlist": "^2.0.0",
    "react": "^16.14.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@storybook/react": "^8.3.6",
    "@swc/core": "^1.7.36",
    "@types/qs": "^6.9.16",
    "@types/react": "^18.3.11",
    "@vitejs/plugin-react": "^4.3.3",
    "@vtaits/filterlist": "^2.3.0",
    "@vtaits/react-router-dom-fake-browser": "^1.0.0",
    "qs": "^6.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "tsup": "^8.3.0",
    "typescript": "5.6.3",
    "vite": "^5.4.9",
    "vitest": "^2.1.3"
  },
  "dependencies": {
    "@vtaits/use-latest": "^1.3.0",
    "is-promise": "^4.0.0"
  }
}
