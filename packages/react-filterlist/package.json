{
  "type": "module",
  "name": "@vtaits/react-filterlist",
  "version": "1.0.0",
  "description": "React wrapper above @vtaits/filterlist",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vtaits/filterlist.git"
  },
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "keywords": [
    "filterlist",
    "filter",
    "sort",
    "pagination",
    "react"
  ],
  "scripts": {
    "build": "tsup src/index.ts --sourcemap --format esm,cjs --dts --legacy-output",
    "lint": "biome check src",
    "lint:fix": "biome check src --apply-unsafe",
    "format": "biome format src --write",
    "test:ts": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:unit:dev": "vitest",
    "test": "yarn test:unit",
    "prepare": "yarn build"
  },
  "peerDependencies": {
    "@vtaits/filterlist": "^0.3.0 || ^1.0.0",
    "react": "^16.14.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@storybook/react": "^8.0.10",
    "@types/qs": "^6.9.15",
    "@types/react": "^18.3.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vtaits/filterlist": "^1.0.0",
    "@vtaits/react-router-dom-fake-browser": "^1.0.0",
    "qs": "^6.12.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "is-promise": "^4.0.0",
    "use-latest": "^1.2.1"
  }
}
