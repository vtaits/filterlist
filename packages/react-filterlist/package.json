{
  "name": "@vtaits/react-filterlist",
  "version": "0.3.1",
  "description": "React wrapper above @vtaits/filterlist",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "ts/index.d.ts",
  "files": [
    "/es",
    "/lib",
    "/ts"
  ],
  "repository": "git@github.com:vtaits/filterlist.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "keywords": [
    "filterlist",
    "filter",
    "sort",
    "pagination",
    "react"
  ],
  "scripts": {
    "clean": "rimraf lib es ts",
    "build:cjs": "cross-env BABEL_ENV=cjs babel --root-mode upward --extensions '.ts,.tsx' src --out-dir lib --ignore \"src/**/__tests__\",\"src/**/__stories__\"",
    "build:es": "cross-env BABEL_ENV=es babel --root-mode upward --extensions '.ts,.tsx' src --out-dir es --ignore \"src/**/__tests__\",\"src/**/__stories__\"",
    "build:ts": "tsc --declaration --emitDeclarationOnly",
    "build": "yarn clean && yarn build:cjs && yarn build:es && yarn build:ts",
    "lint": "eslint src --ext .ts,.tsx --ignore-path ../../.gitignore",
    "test:ts": "tsc -p ./tsconfig.validate.json --noEmit",
    "test:unit": "jest",
    "test": "yarn test:unit",
    "prepare": "yarn build"
  },
  "peerDependencies": {
    "@vtaits/filterlist": "^0.2.3",
    "react": "^16.14.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@storybook/react": "^6.5.13",
    "@types/enzyme": "^3.10.12",
    "@types/history": "^4.7.11",
    "@types/jest": "^29.2.0",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.24",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/parser": "^5.41.0",
    "@vtaits/filterlist": "^0.2.3",
    "@vtaits/react-router-dom-fake-browser": "^0.2.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "history": "^4.10.1",
    "jest": "^29.2.2",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.3.4",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.4",
    "is-promise": "^4.0.0"
  }
}
