{
  "type": "module",
  "name": "@vtaits/react-filterlist",
  "version": "3.0.0",
  "description": "React wrapper above @vtaits/filterlist",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "dist/ts/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/ts/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vtaits/filterlist.git"
  },
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "keywords": [
    "filterlist",
    "filter",
    "sort",
    "pagination",
    "react"
  ],
  "scripts": {
    "build": "bun build.ts && tsgo -p tsconfig.build.json",
    "lint": "biome check src __stories__",
    "lint:fix": "biome check src __stories__ --write --unsafe",
    "format": "biome format src __stories__ --write",
    "test:ts": "tsgo",
    "test:unit": "bun test --coverage",
    "test:unit:dev": "bun test --watch",
    "test": "bun run test:unit",
    "prepare": "bun run build"
  },
  "peerDependencies": {
    "@vtaits/filterlist": "^3.0.0",
    "react": "^16.14.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.2",
    "@happy-dom/global-registrator": "^20.0.10",
    "@storybook/react-vite": "^10.0.0",
    "@swc/core": "^1.13.5",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/qs": "^6.14.0",
    "@types/react": "^19.2.2",
    "@typescript/native-preview": "^7.0.0-dev.20251029.1",
    "@vtaits/filterlist": "^3.0.0",
    "@vtaits/react-router-dom-fake-browser": "^1.0.0",
    "happy-dom": "^20.0.10",
    "qs": "^6.14.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "typescript": "5.9.3",
    "vite": "^7.1.12"
  },
  "dependencies": {
    "@vtaits/use-latest": "^1.3.0"
  }
}
