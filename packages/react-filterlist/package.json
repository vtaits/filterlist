{
  "name": "@vtaits/react-filterlist",
  "version": "0.3.1",
  "description": "React wrapper above @vtaits/filterlist",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "typings": "./dist/index.d.ts",
  "repository": "git@github.com:vtaits/filterlist.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "keywords": [
    "filterlist",
    "filter",
    "sort",
    "pagination",
    "react"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts --sourcemap --format esm,cjs --dts --legacy-output",
    "lint": "eslint src --ext .ts,.tsx --ignore-path ../../.gitignore",
    "test:ts": "tsc --noEmit",
    "test:unit": "jest",
    "test": "yarn test:unit",
    "prepare": "yarn build"
  },
  "peerDependencies": {
    "react": "^16.14.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.21.5",
    "@storybook/react": "^6.5.16",
    "@types/enzyme": "^3.10.13",
    "@types/history": "^4.7.11",
    "@types/jest": "^29.5.1",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.2.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/parser": "^5.59.1",
    "@vtaits/react-router-dom-fake-browser": "^0.2.2",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "history": "^4.10.1",
    "jest": "^29.5.0",
    "qs": "^6.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.3.4",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@types/prop-types": "^15.7.5",
    "@vtaits/filterlist": "^0.3.0",
    "is-promise": "^4.0.0",
    "prop-types": "^15.8.1",
    "use-latest": "^1.2.1"
  }
}
