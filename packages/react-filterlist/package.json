{
  "type": "module",
  "name": "@vtaits/react-filterlist",
  "version": "3.0.0-signals.0",
  "description": "React wrapper above @vtaits/filterlist",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vtaits/filterlist.git"
  },
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "keywords": [
    "filterlist",
    "filter",
    "sort",
    "pagination",
    "react"
  ],
  "scripts": {
    "build": "tsup src/index.ts --sourcemap --format esm --dts",
    "lint": "biome check src",
    "lint:fix": "biome check src --write --unsafe",
    "format": "biome format src --write",
    "test:ts": "tsc --noEmit",
    "test:unit": "bun test --coverage",
    "test:unit:dev": "bun test --watch",
    "test": "bun run test:unit",
    "prepare": "bun run build"
  },
  "peerDependencies": {
    "@vtaits/filterlist": "^3.0.0-signals.0",
    "react": "^16.14.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@happy-dom/global-registrator": "^18.0.1",
    "@storybook/react-vite": "^9.0.8",
    "@swc/core": "^1.12.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/qs": "^6.14.0",
    "@types/react": "^19.1.7",
    "@vtaits/filterlist": "^3.0.0-signals.0",
    "@vtaits/react-router-dom-fake-browser": "^1.0.0",
    "happy-dom": "^18.0.1",
    "qs": "^6.14.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "tsup": "^8.5.0",
    "typescript": "5.8.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@vtaits/react-signals": "^1.0.1",
    "@vtaits/use-latest": "^1.3.0",
    "is-promise": "^4.0.0",
    "signal-polyfill": "^0.2.1"
  }
}
