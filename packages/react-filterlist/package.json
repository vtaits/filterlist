{
  "name": "@vtaits/react-filterlist",
  "version": "0.2.1",
  "description": "React wrapper above @vtaits/filterlist",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "ts/index.d.ts",
  "files": [
    "/es",
    "/lib",
    "/ts"
  ],
  "repository": "git@github.com:vtaits/filterlist.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "keywords": [
    "filterlist",
    "filter",
    "sort",
    "pagination",
    "react"
  ],
  "scripts": {
    "clean": "rimraf lib es ts",
    "build:cjs": "cross-env BABEL_ENV=cjs babel --extensions '.ts' src --out-dir lib --ignore \"src/**/__tests__\",\"src/**/__stories__\"",
    "build:es": "cross-env BABEL_ENV=es babel --extensions '.ts' src --out-dir es --ignore \"src/**/__tests__\",\"src/**/__stories__\"",
    "build:ts": "tsc --declaration",
    "build": "yarn clean && yarn build:cjs && yarn build:es && yarn build:ts",
    "lint": "eslint src --ext .ts,.tsx --ignore-path ../../.gitignore",
    "test:unit": "jest",
    "test": "yarn test:unit",
    "prepare": "yarn build"
  },
  "peerDependencies": {
    "@vtaits/filterlist": "^0.2.0",
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@vtaits/filterlist": "^0.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "is-promise": "^2.1.0",
    "prop-types": "^15.7.2"
  }
}
