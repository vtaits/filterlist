{
  "type": "module",
  "name": "@vtaits/react-filterlist",
  "version": "0.4.0-alpha.1",
  "description": "React wrapper above @vtaits/filterlist",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": "git@github.com:vtaits/filterlist.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "keywords": [
    "filterlist",
    "filter",
    "sort",
    "pagination",
    "react"
  ],
  "scripts": {
    "build": "tsup src/index.ts --sourcemap --format esm,cjs --dts --legacy-output",
    "lint": "rome check src",
    "format": "rome check --apply-unsafe src && rome format --write src",
    "test:ts": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:unit:dev": "vitest",
    "test": "yarn test:unit",
    "prepare": "yarn build"
  },
  "peerDependencies": {
    "@vtaits/filterlist": "^0.3.0",
    "react": "^16.14.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@storybook/react": "^7.4.0",
    "@types/qs": "^6.9.8",
    "@types/react": "^18.2.21",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.0.4",
    "@vtaits/filterlist": "^0.3.0",
    "@vtaits/react-router-dom-fake-browser": "^1.0.0",
    "cross-env": "^7.0.3",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "rome": "^12.1.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  },
  "dependencies": {
    "is-promise": "^4.0.0",
    "use-latest": "^1.2.1"
  }
}
