{
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "repository": "git@github.com:vtaits/filterlist.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "storybook dev -p 6006",
    "build": "yarn workspaces foreach -t run build",
    "test:ts": "yarn workspaces foreach -p run test:ts",
    "lint": "yarn workspaces foreach -p run lint",
    "lint:fix": "biome check examples --apply-unsafe && yarn workspaces foreach -p run lint:fix",
    "format": "biome format --write examples && yarn workspaces foreach -p run format",
    "test:unit": "yarn workspaces foreach -p run test:unit",
    "test": "yarn lint && yarn test:ts && yarn test:unit",
    "validate": "yarn format && yarn build && yarn test",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.3.3",
    "@faker-js/faker": "^8.3.1",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/builder-vite": "^7.5.3",
    "@storybook/manager-api": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@types/react": "^18.2.37",
    "@vitejs/plugin-react": "^4.2.0",
    "@vtaits/react-paginator": "^2.0.0",
    "@vtaits/react-router-dom-fake-browser": "^1.0.0",
    "husky": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "storybook": "^7.5.3",
    "styled-components": "^6.1.1",
    "typescript": "^5.2.2",
    "vite": "5.0.0"
  },
  "packageManager": "yarn@3.6.3"
}
