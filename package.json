{
  "name": "@vtaits/filterlist",
  "version": "0.1.0",
  "description": "Creating lists with filters, sotring, paginatinon, endless scroll etc",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": "git@github.com:vtaits/filterlist.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "keywords": [
    "filterlist",
    "filter",
    "sort",
    "pagination",
    "vanilla"
  ],
  "scripts": {
    "clean": "rm -rf lib es",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib --ignore \"src/**/__tests__\"",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --ignore \"src/**/__tests__\"",
    "build": "yarn clean && yarn build:cjs && yarn build:es",
    "lint": "eslint src",
    "test:unit": "jest",
    "test": "yarn lint && yarn test:unit",
    "prepublish": "yarn build"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.26.2",
    "jest-cli": "^23.6.0"
  },
  "dependencies": {
    "array-insert": "^2.0.0",
    "es6-error": "^4.1.1",
    "fbemitter": "^2.1.1"
  }
}
